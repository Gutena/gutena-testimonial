{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "gutena/testimonial-text",
    "title": "Text",
    "category": "gutena",
    "parent": [ "gutena/testimonial-item" ],
    "description": "Testimonial Text Block by Gutena",
    "textdomain": "gutena-testimonial",
    "supports": {
        "html": false,
        "spacing": {
			"padding": true,
			"margin": true
		},
        "color": {
            "background": false,
            "gradients": false,
            "text": true,
            "enableContrastChecker": false
        }
    },
    "attributes": {
        "textContent": {
            "type": "string"
        }
    },
    "editorScript": "file:./index.js"
}